{
	"name": "pl_publish_data_into_sql",
	"properties": {
		"activities": [
			{
				"name": "sql_copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureDatabricksDeltaLakeSource",
						"exportSettings": {
							"type": "AzureDatabricksDeltaLakeExportCommand"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "ls_datalake_sink",
							"type": "LinkedServiceReference"
						},
						"path": "manualfiles"
					},
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "ls_datalake_sink",
								"type": "LinkedServiceReference"
							},
							"path": "manualfiles"
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "iata_code",
									"type": "String"
								},
								"sink": {
									"name": "iata_code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "icao_code",
									"type": "String"
								},
								"sink": {
									"name": "icao_code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "name",
									"type": "String"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ds_adb_delta_lake",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_database_sink",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}